{
    "name": "alpari/soap-client",
    "description": "Soap client with additional features",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "~7.1",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "symfony/browser-kit": "^4.1",
        "zendframework/zend-soap": "^2.7",
        "phpunit/phpunit": "~7",
        "symfony/process": "^4.2",
        "symfony/http-kernel": "^4.2",
        "mikey179/vfsStream": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Alpari\\Components\\SoapClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alpari\\Components\\SoapClient\\": "tests"
        },
        "files": ["tests/stubs.php"]
    }
}
